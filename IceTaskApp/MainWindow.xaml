<Window x:Class="IceTaskApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IceTaskApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <PasswordBox x:Name="txtPassword" HorizontalAlignment="Left" Margin="294,275,0,0" VerticalAlignment="Top" Width="216" Height="49"/>
        <TextBox x:Name="txtName" HorizontalAlignment="Center" Margin="0,71,0,0" TextWrapping="Wrap" Text="Name" FontSize="25" VerticalAlignment="Top" Width="212" Height="43" TextChanged="textBox_TextChanged"/>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Center" Margin="0,161,0,0" TextWrapping="Wrap" Text="Email" FontSize="25" VerticalAlignment="Top" Width="212" Height="56" TextChanged="txtEmail_TextChanged"/>

    </Grid>
</Window>
